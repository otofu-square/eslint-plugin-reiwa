{
  "name": "eslint-plugin-reiwa",
  "version": "0.0.5",
  "description": "ESLint rules to replace heisei with reiwa",
  "repository": "ssh://git@github.com/otofu-square/eslint-plugin-reiwa.git",
  "keywords": [
    "eslint",
    "reiwa",
    "heisei"
  ],
  "author": "otofu-square <otofu.square@gmail.com>",
  "license": "MIT",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "pack build",
    "eslint": "eslint --cache ./**/*.ts",
    "publish": "pack publish",
    "test": "jest",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@pika/pack": "0.3.6",
    "@pika/plugin-build-node": "0.3.14",
    "@pika/plugin-standard-pkg": "0.4.0",
    "@types/eslint": "4.16.6",
    "@types/jest": "24.0.11",
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "eslint": "5.15.3",
    "eslint-config-prettier": "4.1.0",
    "husky": "4.2.5",
    "jest": "24.8.0",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "ts-jest": "24.0.0",
    "typescript": "3.4.1"
  },
  "peerDependencies": {
    "eslint": "^4.0.0 || ^5.0.0"
  }
}
